@charset "EUC-KR";
html
{
	background-color:#0c0c0c;
	color:#FFFFFF;
	font-family: 'Hahmlet', serif;
}
.loginimg
{
	width:40px;
	height:40px;
	margin:45px 30px 0 70px;
	
}
.userimg
{
	width:40px;
	height:40px;
	margin-top:37px;
}
.searchicon
{
	width:25px;
	height:25px;
	margin-top:40px;
	cursor:pointer;
	margin-right:50px;
}

.logoimg
{
	width:70px;
	height:70px;
	margin-top:17px;
}
.logo2
{
	margin:16px 50px 0 10px;
	font-size:50px;
	font-weight:bold;
}
.searcher
{
	margin:45px 10px 10px 30px;
	width:300px;
	height:30px;
	border-radius:15px;
	padding-left:15px;
	border: 1px solid black;
	background-color:#353535;
	color:white;
}
.header
{
	margin: 0 auto 0 auto;
	display: flex;
 	justify-content: center;
}
.infoimg p
{
	float:right;
	margin:44px 0 0 10px;
}
.menu
{
	margin-top:15px;
	text-align:center;
	font-size:15pt;
	font-weight:bold;
	color:#616161;
}
.menu a:hover
{
	margin-top:15px;
	text-align:center;
	font-size:15pt;
	font-weight:bold;
	color:white;
}
a
{
	text-decoration:none;
	color: inherit;
}
.mymenu p
{
	margin:44px 30px 0 110px;
}

.menu a:nth-child(n)
{
	padding:0 60px 0 60px;
}
.sidead
{
	display:inline-block;
	float:right;
	padding:0px;
	background:#FF6347;
	margin-top:200px;
	width:200px;
	height:600px;
	position:fixed;
	z-index:100;
}
@media all and (min-width: 1101px) 
{
    .sidead 
    {
	    position: sticky;
	    align-self: flex-start;
	    top: 160px;
	    right:150px;
    }
}