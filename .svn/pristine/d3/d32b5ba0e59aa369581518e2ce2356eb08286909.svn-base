@charset "EUC-KR";

.footer
{
	clear:both;
	height:100px;
	background-color:#0c0c0c;
	padding-top:10px;
	width:100%;
}
.footer_frame
{
	width:1000px;
	margin:0 auto 0 auto;
	text-align:center;
}
.footer a
{
	color: inherit;
  	text-decoration: none;
  	
}


.footer a:nth-child(n)
{
	margin:0 20px 0 20px;
}
.footer a:nth-child(1)
{
	text-align:center;
}
.footer a:nth-last-child(1)
{
	text-align:center;
	font-weight:bold;
	font-size:30px;
}