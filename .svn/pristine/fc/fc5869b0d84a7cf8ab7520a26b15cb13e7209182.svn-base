@charset "EUC-KR";
.main
{
	margin-top:20px;
	background-color:#1b1b1b;	
}

.writertap
{
	text-align:center;
}
.writertap td:nth-child(1)
{
	font-weight:bold;
}
.writertap td:nth-child(2)
{
	color:#c4c4c4;
	width:100px;
}
.writertap td:nth-child(3)
{
	color:#808080;
}
.searchtap
{
	width:1000px;
	height:100%;
	margin:0 auto 0 auto;
}
.searchlist
{
	width:100%;
	height:150px;
	background-color:#2e2e2e;
} 
.searchtitle
{
	font-weight:bold;
	font-size:20px;
	padding: 10px 0 0 20px;
}
.searchlist img
{
	float:right;
	width:100%;
	height:100%;
	padding:10px;
}
.searchimage
{
	width:300px;
	height:150px;
}
.searchtext
{
	padding: 0 15px 20px 20px;
}

.b1
{
	border:0;
	background-color:#D9D9D9;
	height:1px;
}
.titletag
{
	font-weight:bold;
	width:1000px;
}
.titletag td:nth-child(1)
{
	font-size:30px;
}
.titletag td:nth-child(2)
{
	font-size:15px;
	text-align:right;
}


